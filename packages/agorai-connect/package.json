{
  "name": "agorai-connect",
  "version": "0.0.6",
  "description": "Lightweight client to connect AI agents to an Agorai bridge",
  "type": "module",
  "license": "AGPL-3.0-only",
  "bin": {
    "agorai-connect": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "agorai",
    "mcp",
    "agent",
    "bridge",
    "claude",
    "ollama",
    "ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenJohnson998/Agorai"
  },
  "homepage": "https://github.com/StevenJohnson998/Agorai/tree/main/packages/agorai-connect#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4"
  }
}
