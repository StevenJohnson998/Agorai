{
  "user": "default",
  "thoroughness": 0.5,
  "agents": [
    {
      "name": "claude",
      "command": "claude",
      "args": ["-p", "--output-format", "json"],
      "personas": ["architect", "security"],
      "enabled": true
    },
    {
      "name": "gemini",
      "command": "gemini",
      "args": ["-p", "--output-format", "json"],
      "personas": ["pragmatist"],
      "enabled": false
    },
    {
      "name": "ollama",
      "model": "qwen3",
      "endpoint": "http://localhost:11434",
      "personas": ["critic"],
      "enabled": false
    },

    {
      "name": "groq-llama",
      "type": "openai-compat",
      "model": "llama-3.3-70b-versatile",
      "endpoint": "https://api.groq.com/openai",
      "apiKey": "gsk_YOUR_GROQ_KEY",
      "personas": ["pragmatist"],
      "enabled": false
    },
    {
      "name": "mistral",
      "type": "openai-compat",
      "model": "mistral-small-latest",
      "endpoint": "https://api.mistral.ai",
      "apiKey": "YOUR_MISTRAL_KEY",
      "personas": [],
      "enabled": false
    },
    {
      "name": "deepseek",
      "type": "openai-compat",
      "model": "deepseek-chat",
      "endpoint": "https://api.deepseek.com",
      "apiKey": "YOUR_DEEPSEEK_KEY",
      "personas": ["critic"],
      "enabled": false
    },
    {
      "name": "lmstudio",
      "type": "openai-compat",
      "model": "loaded-model",
      "endpoint": "http://localhost:1234",
      "personas": [],
      "enabled": false
    }
  ],
  "personas": [
    {
      "name": "devops",
      "role": "DevOps Engineer",
      "systemPrompt": "You are a DevOps engineer. Focus on deployment, CI/CD, infrastructure, monitoring, and operational concerns. Evaluate proposals for production-readiness.",
      "consensusBonus": 1.0
    }
  ],
  "budget": {
    "maxTokensPerDebate": 0,
    "maxTokensPerProject": 0,
    "warnAtPercent": 80,
    "estimatedTokensPerInvocation": 1500
  },
  "logging": {
    "info": {
      "purge": "date",
      "maxDays": 30,
      "maxBytes": 52428800
    },
    "debates": {
      "purge": "count",
      "maxFiles": 50,
      "maxDays": 14,
      "maxBytes": 104857600
    }
  },
  "database": {
    "path": "./data/agorai.db"
  },
  "bridge": {
    "port": 3100,
    "host": "127.0.0.1",
    "apiKeys": [
      {
        "key": "ak_desktop_CHANGE_ME",
        "agent": "claude-desktop",
        "type": "claude-desktop",
        "capabilities": ["analysis", "review"],
        "clearanceLevel": "team"
      },
      {
        "key": "ak_code_CHANGE_ME",
        "agent": "claude-code",
        "type": "claude-code",
        "capabilities": ["code-execution", "testing"],
        "clearanceLevel": "confidential"
      }
    ]
  }
}
